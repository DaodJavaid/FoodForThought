@section Scripts {
    <script>
        $(document).ready(function () {
            $(".header").hide();
            $("#footer").hide();
        });
    </script>
}


<!-- Begin page -->
<div id="layout-wrapper">

    @Html.Partial("_UserVerticalMenu")
    <!-- ============================================================== -->
    <!-- Start right Content here -->
    <!-- ============================================================== -->
    <div class="main-content">

        <div class="page-content">
            <div class="container-fluid">

                <!-- start page title -->
                <div class="row">
                    <div class="col-12">
                        <div class="page-title-box d-flex align-items-center justify-content-between">
                            <h4 class="mb-0 font-size-18">Show User</h4>

                            <div class="page-title-right">
                                <ol class="breadcrumb m-0">
                                    <li class="breadcrumb-item"><a href="javascript: void(0);">Food For Thought</a></li>
                                    <li class="breadcrumb-item active">Show User</li>
                                </ol>
                            </div>

                        </div>
                    </div>
                </div>
                <!-- end page title -->
                <!-- Update Form -->
                <div class="row">

                    <div class="col-lg-12">
                        <div class="card card-animate" id="user_background">
                            <div class="card-body">
                                <div id="morris-line-example" class="morris-chart" style="height: 320px;">

                                    <form method="post" id="Register_form" >
                                        <div class="form-row">
                                            <div class="form-group col-md-6">
                                                <label for="inputEmail4">First Name</label>
                                                <input type="text" name="username" class="form-control" id="inputfirstname" placeholder="First Name" value="@TempData["UserName"]" readonly>
                                            </div>
                                            <div class="form-group col-md-6">
                                                <label for="inputEmail4">Email</label>
                                                <input type="email" name="email_old" class="form-control" id="inputEmail4" placeholder="Email" value="@TempData["UserEmail"]" readonly>
                                            </div>
                                        </div>
                                         <div class="form-group">
                                            <label for="inputAddress">Address</label>
                                            <textarea type="text" name="address" class="form-control" id="inputAddress" placeholder="1234 Main St" readonly>@TempData["UserAddress"]</textarea>
                                        </div>
                                        <div class="form-row">
                                            <div class="form-group col-md-5">
                                                <label for="inputCountry">Country</label>
                                                <input type="text" name="country" class="form-control" id="inputCountry" value="@TempData["UserCountry"]" readonly>
                                            </div>

                                            <div class="form-group col-md-5">
                                                <label for="inputCity">City</label>
                                                <input type="text" name="city" class="form-control" id="inputCity" value="@TempData["UserCity"]" readonly>
                                            </div>

                                            <div class="form-group col-md-1">
                                                <label for="inputZip">Zip</label>
                                                <input type="text" name="zip" class="form-control" id="inputZip" value="@TempData["UserZip"]" readonly>
                                            </div>
                                        </div>
                                        <div class="form-row">
                                            <div class="form-group col-md-5">
                                                <label for="inputCountry">Gender</label>
                                                <input type="text" name="country" class="form-control" id="inputCountry" value="@TempData["UserGender"]" readonly>
                                            </div>
                                        </div>
                                    </form>
                                </div>
                            </div>
                        </div>
                    </div> <!-- end col -->
                    <!-- end row-->

                </div> <!-- container-fluid -->
            </div>
            <!-- End Page-content -->

        </div>
        <!-- end main content-->

    </div>
    <!-- END layout-wrapper -->
    <!-- Overlay-->
    <div class="menu-overlay"></div>
</div>